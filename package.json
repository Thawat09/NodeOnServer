{
  "name": "nodeonserver",
  "version": "1.0.0",
  "description": "Test Node on Server",
  "main": "index.js",
  "scripts": {
    "start": "index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "configure-husky": "npx husky install && npx husky add .husky/pre-commit \"npm run fig\"",
    "fig": "eslint .",
    "build": "babel src -d dist",
    "prepare": "husky install",
    "lint": "eslint .",
    "format": "prettier --write .",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "npm run format",
      "git add"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@line/bot-sdk": "^9.2.0",
    "async-redis": "^2.0.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "moment": "^2.30.1",
    "morgan": "^1.10.0",
    "passport": "^0.7.0",
    "passport-http": "^0.3.0",
    "passport-local": "^1.0.0",
    "pg": "^8.11.5",
    "pg-hstore": "^2.3.4",
    "redis": "^4.6.13",
    "sequelize": "^6.37.2",
    "strict-transport-security": "^0.3.0",
    "tedious": "^18.1.0",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@types/node": "20.11.5",
    "eslint-plugin-jest": "^28.2.0",
    "gts": "^5.3.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "typescript": "~5.4.3"
  }
}
